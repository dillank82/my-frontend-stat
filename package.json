{
  "name": "my-frontend-stat-project",
  "version": "1.0.0",
  "scripts": {
    "build": "pnpm --filter server build",
    "dev:server": "pnpm --filter server start",
    "dev:app": "pnpm --filter app dev",
    "dev": "npm-run-all --parallel dev:server dev:app", 
    "start": "pnpm build && pnpm dev"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "typescript-eslint": "^8.48.0"
  },
  "workspaces": [
    "server",
    "app"
  ],
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}